import "printf", "strcmp" from "libc.so.6";

function log(val)
{
	var type = typeof val;
	printf("%s - ", type);

	if(strcmp(type, "int") == 0)
		printf("%ld\n", val);
	else if(strcmp(type, "float") == 0)
		printf("%f\n", val);
	else if(strcmp(type, "string") == 0)
		printf("%s\n", val);
	else
		printf("%p\n", val);
}

log(1337 * 42);
log(3.1415);
log("Hello world!");
log(log);
