import "printf", "strcmp" from "libc.so.6";

function log(val)
{
	var type = typeof val;
	printf("%s - ", type);

	if(type == "int")
		printf("%ld\n", val);
	else if(type == "float")
		printf("%g\n", val);
	else if(type == "string")
		printf("%s\n", val);
	else
		printf("%p\n", val);
}

log(1337 * 42);
log(3.1415);
log("Hello world!");
log(log);
