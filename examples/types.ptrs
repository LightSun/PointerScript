import "printf", "strcmp" from "libc.so.6";

function log(val)
{
	var type = typeidof val;
	printf("%s - ", typeof val);

	if(type == int)
		printf("%ld\n", val);
	else if(type == float)
		printf("%g\n", val);
	else if(type == native)
		printf("%s\n", val);
	else
		printf("%p\n", val);
}

log(1337 * 42);
log(3.1415);
log("Hello world!");
log(log);
