import printf, strcmp, ptrs_typetoa;

function log(val)
{
	var typeid = typeof val;
	printf("%s - ", ptrs_typetoa(typeid));

	if(typeid == type<int>)
		printf("%ld\n", val);
	else if(typeid == type<float>)
		printf("%g\n", as<float>val);
	else if(typeid == type<native>)
		printf("%s\n", val);
	else
		printf("%p\n", val);
}

log(1337 * 42);
log(3.1415);
log("Hello world!");
