import "printf", "asprintf", "free", "ptrs_typetoa";

struct Foo
{
	bar;
	tar;
	hololo()
	{
		this.bar += this.tar;
		this.dump();
	}
	dump()
	{
		printf("%-20s%-20s%-20s\n", ptrs_typetoa(typeof this), ptrs_typetoa(typeof this.bar), ptrs_typetoa(typeof this.tar));
		printf("%-20p%-20g%-20g\n", this, cast<float>this.bar, cast<float>this.tar);
	}
};

var bla = new Foo();
bla.bar = 42;
bla.tar = 3.1415;
bla.dump();
bla.hololo();
free(bla);
