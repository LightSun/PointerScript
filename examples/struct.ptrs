import "printf", "asprintf", "free" from "libc.so.6";

struct Foo
{
	bar;
	tar;
	hololo()
	{
		this.bar += this.tar;
		this.dump();
	}
	dump()
	{
		printf("%-20s%-20s%-20s\n", typeof this, typeof this.bar, typeof this.tar);
		printf("%-20p%-20g%-20g\n", this, (float)this.bar, (float)this.tar);
	}
};

var bla = new Foo();
bla.bar = 42;
bla.tar = 3.1415;
bla.dump();
bla.hololo();
free(bla);
